--EXERCÍCIO 1
SELECT * FROM SCOTT.EMP;

--EXERCÍCIO 2
SELECT * FROM SCOTT.DEPT;

--EXERCÍCIO 3
SELECT ENAME, HIREDATE FROM SCOTT.EMP;

--EXERCÍCIO 4
SELECT DNAME, LOC FROM SCOTT.DEPT;

--EXERCÍCIO 5
SELECT ENAME, JOB, SAL FROM SCOTT.EMP;

--EXERCÍCIO 6
SELECT * FROM SCOTT.EMP WHERE SAL > 1500;

--EXERCÍCIO 7
SELECT * FROM SCOTT.EMP WHERE SAL <=2000;

--EXERCÍCIO 8
SELECT * FROM SCOTT.EMP WHERE EMPNO = 7902;

--EXERCÍCIO 9
SELECT * FROM SCOTT.EMP WHERE SAL BETWEEN 500 AND 1800;

--EXERCÍCIO 10
SELECT * FROM SCOTT.DEPT WHERE DEPTNO = 30;

--EXERCÍCIO 11
SELECT EMPNO, SAL, SAL + 500 FROM SCOTT.EMP;

-- EXERCÍCIO 12
SELECT ENAME, JOB, SAL FROM SCOTT.EMP WHERE ENAME = 'KING';

--EXERCÍCIO 13
SELECT * FROM SCOTT.EMP WHERE COMM IS NULL;

--EXERCÍCIO 14
SELECT ENAME, SAL, JOB FROM SCOTT.EMP WHERE ENAME LIKE 'M%';

--EXERCÍCIO 15
SELECT * FROM SCOTT.EMP WHERE JOB = 'SALESMAN';

--EXERCÍCIO 16
SELECT * FROM SCOTT.EMP WHERE DEPTNO = 10;

--EXERCÍCIO 17
SELECT * FROM SCOTT.DEPT WHERE LOC = 'CHICAGO';

--EXERCÍCIO 18
SELECT * FROM SCOTT.EMP WHERE ENAME LIKE '%S';


